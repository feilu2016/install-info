    service mysql restart
    cat /etc/my.cnf
    mysqladmin -u root password 'yeelion'
    firewall-cmd --zone=public --add-port=3306/tcp --permanent;
    service iptables stop;
    mysql -h47.92.97.76 -uroot -pyeelion
